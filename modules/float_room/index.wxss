.float-room-container {
  position: fixed;
  height: 40px;
  width: 94px;
  background-color: #2f2f2f;
  border-radius: 20px 0 0 20px;
  top: 12px;
  right: 0;
  overflow: hidden;
}

.float-room-container-active {
  background-color: #1f1f1f;
}

.float-room-icon {
  margin: 10px 10px 10px 13px;
  height: 20px;
  width: 20px;
  float: left;
}

.float-room-text {
  line-height: 42px;
  color: #f8f8f8;
  height: 40px;
}